pipeline{
    agent any 
    
    stages{
        stage('cleanup workspace') {
            steps{
                script{
                    cleanWs()

                }
            }
            stages{
                stage('Checkout SCM') {
                    steps{
                        script{
                            checkout scmGit(branches: [[name: '*/main']],\
                            extensions: [], \
                            userRemoteConfigs: [[url: 'https://github.com/mshow1980/Scion-Scope']])

                        }
                    }
                }
            }
        }
    }
}